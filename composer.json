{
    "name": "stubbles/input",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Stubbles Input enables to handle input data via validation and filtering.",
    "require": {
        "php": ">=5.4.0",
        "stubbles/core": "~4.0"
    },
    "require-dev": {
        "mikey179/vfsStream": "~1.3",
        "phpunit/phpunit": "~4.1",
        "stubbles/date": "~4.0"
    },
    "suggest": {
        "stubbles/date": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\input\\": "src/main/php"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
