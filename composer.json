{
    "name": "stubbles/input",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Stubbles Input enables to handle input data via validation and filtering.",
    "require": {
        "php": ">=5.4.0",
        "stubbles/core": "^6.0"
    },
    "require-dev": {
        "mikey179/vfsStream": "^1.5",
        "phpunit/phpunit": "^4.6",
        "bovigo/callmap": "^1.0",
        "stubbles/date": "^5.3"
    },
    "suggest": {
        "stubbles/date": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\input\\": "src/main/php"
        }
    },
    "autoload-dev": {
        "files": [
            "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
